{
  "name": "bars",
  "version": "1.0.0",
  "type": "commonjs",
  "private": true,
  "description": "Bars a  CLI tool to generate templates for your project",
  "main": "dist/index.js",
  "bin": {
    "bar-kits": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prepublishOnly": "npm run build",
    "plop": "plop"
  },
  "keywords": [
    "cli",
    "typescript"
  ],
  "author": "Trident Technologies",
  "license": "MIT",
  "engines": {
    "node": ">=14.16"
  },
  "dependencies": {
    "@repo/ui": "*",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "figlet": "^1.6.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^8.2.5",
    "nanospinner": "^1.1.0",
    "plop": "^4.0.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/boxen": "^3.0.1",
    "@types/figlet": "^1.5.6",
    "@types/gradient-string": "^1.1.2",
    "@types/inquirer": "^8.2.6",
    "@types/node": "^18.16.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
